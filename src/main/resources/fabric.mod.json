{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_authors}"
  ],
  "contributors": [
    "${mod_credits}"
  ],
  "contact": {
    "sources": "${mod_url}",
    "issues": "${mod_tracker}"
  },
  "license": [
    "${mod_license}"
  ],
  "icon": "${mod_id}.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.aetherteam.protect_your_moa.ProtectYourMoa"
    ],
    "client": [
      "com.aetherteam.protect_your_moa.client.ProtectYourMoaClient"
    ]
  },
  "mixins": [
    "${mod_id}.mixins.json"
  ],
  "depends": {
    "fabricloader": "${fabric_loader_version_range}",
    "minecraft": "${minecraft_version_range}",
    "java": ">=21",
    "fabric-api": "${fabric_version_range}",
    "aether": "${aether_version_range}"
  }
}